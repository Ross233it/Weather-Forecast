name: dockercompose599963739448213037
services:
  weatherforecast:
    container_name: WeatherForecastWebClient
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      ASPNETCORE_URLS: http://+:80;https://+:443
    image: your_docker_registry/weatherforecast:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1116
        published: "80"
        protocol: tcp
      - mode: ingress
        target: 1115
        published: "443"
        protocol: tcp
  wssoaptranslator:
    build:
      context: C:\C-Sharp-Projects\TrentinoWeatherForecast
      dockerfile: WsSoapTranslator/Dockerfile
    container_name: WeatherForecastSoapWs
    image: wssoaptranslator
    networks:
      default: null
    ports:
      - mode: ingress
        target: 2350
        published: "1118"
        protocol: tcp
networks:
  default:
    name: dockercompose599963739448213037_default